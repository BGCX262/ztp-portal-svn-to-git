CREATE TABLE Thread ( 
	id int8 NOT NULL,
	thread_name varchar(255) NOT NULL,
	owner int8 NOT NULL,
	import_date timestamp,
	state boolean,
	active boolean,
	theme int8 NOT NULL,
	change_date timestamp,
	primary key(ID)
);


CREATE TABLE Comment ( 
	id int8 NOT NULL,
	comment_name varchar(512) NOT NULL,
	owner int8 NOT NULL,
	import_date timestamp,
	state boolean,
	active boolean,
	thread int8,
	change_date timestamp,
	notice int8 DEFAULT 0,
	event int8,
	information int8,
	healthAid int8,
	primary key(ID)
);


CREATE TABLE Theme ( 
	id int8 NOT NULL,
	name varchar(255) NOT NULL,
	import_date timestamp,
	change_date timestamp,
	state boolean,
	primary key(ID)
);

