	primary key (ID)
)
;

CREATE TABLE Day_care ( 
	id int8 NOT NULL,
	TYPE_OF_ADVERT int8 NOT NULL,
	HANDICAP_TYPE int8 NOT NULL,
	COUNTY int8 NULL,
