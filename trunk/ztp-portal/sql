CREATE TABLE Events ( 
	id int8 NOT NULL,
	name varchar(255),
	user_id int8,
	type int8,
	town varchar(255),
	county int8,
	address varchar(255),
	start_date timestamp,
	end_date timestamp,
	events_length int8,
	notes varchar(5000),
	guest_count int8,
	state boolean,
	active boolean,
	import_date timestamp,
	change_date timestamp,
	primary key(ID)
);

CREATE TABLE EVENT_TYPE(
id int8,
name varchar(255),
primary key (ID)
);

CREATE TABLE PICTURE(
id int8,
